<?xml version="1.0" encoding="utf-8" ?>
<toolbarapplet on_load="onToolbarAppletLoad" on_click="onButtonClick" buttonstyle="windows" buttonsize="26,25">
	<toolbar id="viewerbar">
		<button image="../images/Save.svg" id="save_report" tooltip="Save Report" disabled="true"/>
		<separator/>
		<button image="../images/Print.svg" id="print_report" tooltip="Print" disabled="true"/>
		<separator/>
		<button image="../images/PDF.svg" id="export_pdf" tooltip="Export as PDF" disabled="true"/>
		<button image="../images/ExportToExcel.svg" id="export_excel" tooltip="Export as Excel" disabled="true"/>
		<button image="../images/ExportToWord.svg" id="export_word" tooltip="Export as Word" disabled="true"/>
		<button image="../images/CSV.svg" id="export_csv" tooltip="Export as CSV" disabled="true"/>
		<button image="../images/XML.svg" id="export_xml" tooltip="Export as Xml" disabled="true"/>
		<button image="../images/SQL.svg" id="export_sql" tooltip="Export as SQL" disabled="true"/>
		<separator/>
		<choice width="auto" id="results" tooltip="Maximum number of results" text="Results" disabled="true">
			<choiceitem id="1">1 Result</choiceitem>
			<choiceitem id="10">10 Results</choiceitem>
			<choiceitem id="100">100 Results</choiceitem>
			<choiceitem id="1000">1000 Results</choiceitem>
			<choiceitem id="10000">10000 Results</choiceitem>
			<choiceitem id="-1">All Results</choiceitem>
		</choice>
		<separator/>
		<button image="../Modules/aras.innovator.Izenda/Images/UnlockAndClose.svg" id="unlock_close" tooltip="Unlock &amp; Close" disabled="true"/>
		<separator/>
		<button image="../images/Help.svg" id="help" tooltip="Help"/>
		<separator/>
		<button image="../images/SaveUnlockClose.svg" id="save_unlock_close" tooltip="Save, Unlock &amp; Close" disabled="true"/>
	</toolbar>
</toolbarapplet>